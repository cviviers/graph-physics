{
    "dataset": {
        "extension": "h5",
        "h5_path": "/data/dataset/h5_dataset/deforming_plate/train.h5",
        "meta_path": "dataset_config/plate_meta.json",
        "khop": 1,
        "new_edges_ratio": 0
    },
    "model": {
        "type": "transformer",
        "message_passing_num": 10,
        "node_input_size": 6,
        "edge_input_size": 0,
        "output_size": 3,
        "hidden_size": 64,
        "num_heads": 4
    },
    "index": {
        "feature_index_start": 0,
        "feature_index_end": 6,
        "output_index_start": 0,
        "output_index_end": 3,
        "node_type_index": 6
    },
    "transformations": {
        "preprocessing": {
            "noise": 0.003,
            "noise_index_start": [0],
            "noise_index_end": [3],
            "masking": 0
        },
        "world_pos_parameters": {
            "use": true,
            "world_pos_index_start": 0,
            "world_pos_index_end": 3
        }
    }
}